package practiceCoding;
import java.util.Scanner;
public class PracticeHacker{
public static void main(String args[]) {
	Scanner sc = new Scanner(System.in);
	int k = sc.nextInt();
	int result = fact(k);
	System.out.println(result);
}
public static int fact(int n) {
	if (n>0) {
		return n*(fact(n-1));
	}
	else {
		return 1;
	}
}
}
