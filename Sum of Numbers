input :               output:
452                   11


code:

package nptelJava;
import java.util.Scanner;
public class NPTELJava {

	public static void main(String args[]) {
		Scanner sc= new Scanner(System.in);
		int n = sc.nextInt();
		int temp = n;
		int result = 0;
		while(n>1) {
			int val = n%10;
			result = val + result;
			n = n/10;
			
		}
		System.out.println("sum of " + temp +"="+result );
	}

}
